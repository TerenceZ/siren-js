{
  "name": "siren-js",
  "version": "0.1.0",
  "description": "A koa-based Node.js web application bootstrapping module, based on kraken-js.",
  "keywords": [
    "node.js",
    "node",
    "javascript",
    "js",
    "web",
    "koa"
  ],
  "main": "index.js",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "test": "mocha --harmony --require should --reporter spec --recursive",
    "test-win": "mocha --harmony --require should --reporter spec --recursive",
    "test-cov": "node --harmony ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require should --recursive",
    "test-cov-win": "node --harmony ./node_modules/istanbul-harmony/lib/cli cover ./node_modules/mocha/bin/_mocha -- --require should --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerenceZ/siren-js.git"
